{
    "name": "Tartan-C-RN",
    "dataset": "chairs",
    "val_dataset": "spring",
    "val_batch_size": 8,
    "gpus": [0, 1, 2, 3, 4, 5, 6, 7],

    "use_var": true,
    "var_min": 0,
    "var_max": 10,
    "pretrain": "resnet34",
    "initial_dim": 64,
    "block_dims": [64, 128, 256],
    "radius": 4,
    "dim": 128,
    "num_blocks": 2,
    "iters": 4,

    "image_size": [368, 496],
    "scale": 0,
    "batch_size": 16,
    "epsilon": 1e-8,
    "lr": 2.5e-4,
    "wdecay": 1e-4,
    "dropout": 0,
    "clip": 1.0,
    "gamma": 0.8,
    "num_steps": 100000,

    "restore_ckpt": "checkpoints/Tartan-RN_exp1.pth",
    "coarse_config": null,
    "log_dir": "logs/",
    "disable_cost": false,

    "extractor": {
        "type": "resnet"
    }
}
